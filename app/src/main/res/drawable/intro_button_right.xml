<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/setupBackground"></solid>
        </shape>
    </item>
    <item android:top="1dp">
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="@color/setupPrimaryDark"></solid>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/setupPrimary"></solid>
                </shape>
            </item>
        </selector>
    </item>
</layer-list>